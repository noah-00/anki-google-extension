{
  "manifest_version": 3,
  "name": "Anki Quicker",
  "version": "1.1.1",
  "description": "Anki Quicker was developed for users of the study support application called Anki.",
  "icons": {
    "16": "/icons/icon16.png",
    "32": "/icons/icon32.png",
    "48": "/icons/icon48.png",
    "192": "/icons/icon192.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "host_permissions": ["http://127.0.0.1:8765/"],
  "permissions": ["storage"]
}
